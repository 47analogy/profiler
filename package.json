{
  "name": "cleopatra",
  "version": "0.0.1",
  "description": "Cleopatra, the gecko profiler UI",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "start": "npm run server",
    "start-prod": "npm run build-prod && npm run plain-server",
    "start-prod-readable": "npm run build-prod-readable && npm run plain-server",
    "server": "NODE_ENV=development node server.js",
    "plain-server": "live-server --port=4242",
    "build": "NODE_ENV=development webpack",
    "build-prod-readable": "NODE_ENV=production webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p --progress",
    "eslint": "eslint index.js src",
    "eslint-fix": "eslint --fix index.js src",
    "documentation": "documentation"
  },
  "author": "Markus Stange <mstange@themasta.com>",
  "license": "MIT",
  "devDependencies": {
    "array-move": "^1.0.0",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babelify": "^7.2.0",
    "bisection": "0.0.3",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "clamp": "^1.0.1",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.9.0",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "live-server": "^1.0.0",
    "memoize-immutable": "^2.1.0",
    "memoizesync": "^0.5.0",
    "mocha": "^2.3.4",
    "npm-run-all": "^2.1.1",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.0",
    "react-redux": "^4.4.2",
    "react-router-redux": "^4.0.2",
    "react-virtualized": "^7.0.5",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1",
    "shallowequal": "^0.2.2",
    "uglify": "^0.1.5",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-es2015-block-scoping",
      "react-hot-loader/babel"
    ]
  }
}
