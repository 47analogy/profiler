{
  "name": "cleopatra",
  "version": "0.0.1",
  "description": "Cleopatra, the gecko profiler UI",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "start": "npm run build && npm run server",
    "server": "live-server www --port=4242",
    "build": "browserify src/index.js -t babelify -o www/bundle.js",
    "watch": "watchify src/index.js -t babelify -o www/bundle.js & watchify src/symbol-store-db-worker.js -t babelify -o www/symbol-store-db-worker.js",
    "build-ugly": "browserify src/index.js -t babelify -o 'uglifyjs -cm > www/bundle.js'",
    "watch-ugly": "watchify src/index.js -t babelify -o 'uglifyjs -cm > www/bundle.js'"
  },
  "author": "Markus Stange <mstange@themasta.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "bisection": "0.0.3",
    "browserify": "^12.0.1",
    "chai": "^3.5.0",
    "live-server": "^0.8.2",
    "mocha": "^2.3.4",
    "uglify": "^0.1.5",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
